{
  "version": 2,
  "builds": [
    {
      "src": "vercel_api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_api.py"
    }
  ],
  "functions": {
    "vercel_api.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
